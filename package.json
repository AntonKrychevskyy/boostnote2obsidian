{
  "name": "boostnote2obsidian",
  "description": "Convert Boostnote-Local to markdown files compatible with Obsidian.",
  "version": "0.0.1",
  "repository": "AntonKrychevskyy/boostnote2obsidian",
  "license": "MIT",
  "main": "bin/index.js",
  "type": "module",
  "bin": {
    "b2o": "bin/cli.js"
  },
  "files": [
    "bin/cli.js",
    "bin/boostnote2obsidian.js",
    "bin/constants.js"
  ],
  "keywords": [
    "convert",
    "Boostnote",
    "markdown"
  ],
  "author": "Anton Krychevskyy",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "test": "mocha",
    "lint": "standard",
    "clean": "del ./bin/**/*",
    "build": "tsc -p ./tsconfig.json",
    "rebuild": "npm run clean && npm run build",
    "debug": "nodemon --inspect ./src/b2o.ts"
  },
  "devDependencies": {
    "@tsconfig/node16-strictest-esm": "^1.0.3",
    "@types/fs-extra": "^11.0.1",
    "coffee": "^5.4.0",
    "del-cli": "^5.0.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.4",
    "standard": "^12.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "fs-extra": "^7.0.1",
    "meow": "^11.0.0",
    "ora": "^3.4.0",
    "sanitize-filename": "^1.6.3"
  }
}
